{
  "name": "扇贝小作业",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "扇贝小作业",
  "icons": {
    "48": "img/logo.png"
  },
  "permissions": [
    "*://api.shanbay.com/*"
  ],
  "browser_action":{
    "default_icon": "img/logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "bower_components/jquery/dist/jquery.min.js",
        "js/content.js"
      ],
      "css": [
        "css/main.css"
      ]
    }
  ]
}


